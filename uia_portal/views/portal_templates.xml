    <template id="portal_administrativo_gestion_vacaciones_aprobar_jefatura_home" name="My Portal UC">
        <t t-call="portal.portal_layout">
            <t t-set="my_details" t-value="True"/>
            <div class="uia_portal_administrativo_gestion_vacaciones_aprobar_jefatura">
                <div class="oe_structure" id="oe_structure_portal_my_home_1"/>
                <h3>Gestión de Vacaciones</h3>
                <div class="o_portal_docs list-group" id="uia_portal_menu_administrativo_vacaciones_aprobar_jefatura_container">
                    <div class="table-responsive">
                        <table class="table" >
                            <thead>
                                <tr>
                                    <th scope="col">Colaborador</th>
                                    <th scope="col">Desde</th>
                                    <th scope="col">Hasta</th>
                                    <th scope="col"></th>
                                    <th scope="col"></th>
                                </tr>
                            </thead>
                            <tbody>
                                <t t-foreach="vacacionesDetail" t-as="line">
                                    <tr t-att-id="line['id']">
                                        <td class="text-center"><t t-esc="line['nombre']" /></td>
                                        <td class="text-center"><t t-esc="line['desde']" /></td>
                                        <td class="text-center"><t t-esc="line['hasta']" /></td>
                                        <td>
                                            <button type="button" id="aceptar_vacaciones" t-att-value="line['id']" class="btn btn-success">Aceptar</button>
                                        </td>
                                        <td>
                                            <button type="button"  id="rechazar_vacaciones" t-att-value="line['id']" class="btn btn-danger">Rechazar</button>
                                        </td>
                                    </tr>

                                </t>

                            </tbody>
                        </table>
                    </div>
                </div>
                <div class="o_portal_docs list-group" id="notificaciones_container">
                </div>
            </div>
            <div class="oe_structure" id="oe_structure_portal_my_home_2"/>
        </t>
    </template>

    <template id="portal_aprobacion_vacaciones" name="My Portal UC">
