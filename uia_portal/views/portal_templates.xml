    <template id="portal_administrativo_gestion_vacaciones_accion_jefatura_inmediata" name="My Portal UC">
        <t t-call="website.layout">
            <div class="oe_structure">
                <div class="container">
                    <div class="portal_administrativo_gestion_vacaciones_accion_jefatura_inmediata_form">
                        <div class="oe_structure" id="oe_structure_portal_my_home_1"/>
                        <h3>Aprobación de vacaciones</h3>
                        <t t-if="accionRealizada == false" >
                            <div class="o_portal_docs list-group" id="portal_administrativo_gestion_vacaciones_accion_jefatura_inmediata_container">

                                <form>
                                    <div class="form-row">
                                        <div class="form-group col-md-6">
                                            <label>Tipo</label>
                                            <input type="text" class="form-control" id="tipo" t-att-value="tipo" readonly="" />
                                        </div>
                                        <div class="form-group col-md-6">
                                            <label>Nombre Administrativo</label>
                                            <input type="text" class="form-control" id="nombre_empleado" t-att-value="nombre_empleado" readonly="" />
                                        </div>
                                        <table class="table">
                                            <thead>
                                                <tr>
                                                    <th class="text-center" scope="col">Desde</th>
                                                    <th class="text-center" scope="col">Hasta</th>
                                                    <th class="text-center" scope="col">Medio día</th>
                                                    <th class="text-center" scope="col">Jornada</th>
                                                    <th class="text-center" scope="col">Cant Días</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr t-foreach="vacacionesList" t-as="line">
                                                    <td class="text-center"><t t-esc="line['dt_desde']" /></td>
                                                    <td class="text-center"><t t-esc="line['dt_hasta']" /></td>
                                                    <td class="text-center"><t t-esc="line['fechaMedioDia']" /></td>
                                                    <td class="text-center"><t t-esc="line['tipoMedioDia']" /></td>
                                                    <td class="text-center"><t t-esc="line['cantDias']" /></td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </form>
                                <form>
                                    <t t-if="diaMedio == true" >
                                        <form>
                                            <div class="form-row">
                                                <div class="form-group col-md-6">
                                                    <label>Fecha Medio Dia</label>
                                                    <input type="text" class="form-control" id="fechaMedioDia" t-att-value="fechaMedioDia" readonly="" />
                                                </div>
                                                <div class="form-group col-md-6">
                                                    <label>Jornada</label>
                                                    <input type="text" class="form-control" id="tipoMedioDia" t-att-value="tipoMedioDia" readonly="" />
                                                </div>
                                            </div>
                                        </form>
                                    </t>

                                    <div class="form-group">
                                        <label for="razon_licencia">Razón</label>
                                        <textarea class="form-control" id="razon_licencia" t-att-value="txt_razon" t-esc="txt_razon"  rows="3" readonly="" ></textarea>
                                    </div>
                                    <div class="form-group row">
                                        <div class="col-sm-10">
                                            <button type="button" id="btn_aceptar_vacaciones" class="btn btn-primary">Aceptar</button>
                                            <button type="button" id="btn_rechazar_vacaciones" class="btn btn-danger">Rechazar</button>
                                        </div>
                                    </div>
                                </form>
                            </div>
                            <div class="o_portal_docs list-group" id="notificaciones_container">
                            </div>
                        </t>

                        <t t-if="accionRealizada == true" >
                            <div class="o_portal_docs list-group" id="notificaciones_container">
                                <div class="alert alert-warning" role="alert">
                                    Ya sé a realizado una acción sobre esta solicitud de vacaciones.
                                </div>
                            </div>
                        </t>

                    </div>
                    <div class="oe_structure" id="oe_structure_portal_my_home_2"/>
                    <p style="visibility: hidden" id="id_vacaciones"  t-esc="idVacaciones"/>
                </div>
            </div>
        </t>
    </template>

